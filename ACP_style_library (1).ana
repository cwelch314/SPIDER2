  Metavar objs := ACP_Orderobjs(c, method);~
  Metavar o[] := objs Do BEGIN~
     IF ACP_Isamodule(o)~
     THEN(~
           IF o <> ACP_Excluded_module~
           THEN res := Concat(res, ACP_Objs_ordered_by_(o, method))~
           ELSE 0;~
     )~
     ELSE res := Concat(res, [o]);~
  END; ~
  res~
END
